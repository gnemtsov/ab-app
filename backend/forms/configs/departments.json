{
	"title": {
		"type": "string",
		"validators": {
			"both": {
				"length": [1,50]
			} 
		},
		"db_table": "departments",
		"db_column": "d_title"
	},
	"head": {
		"type": "string",
		"nullable": true,
		"validators": {
			"both": {
				"length": [1,50]
			} 
		},
		"db_table": "departments",
		"db_column": "d_head"
	},
	"size": {
		"type": "number",
		"validators": {
			"both": {
				"greaterOrEqual": 0
			} 
		},
		"db_table": "departments",
		"db_column": "d_size"
	},
	"created": {
		"type": "date",
		"nullable": true,
		"validators": {
			"both": {
				"after": "1900-01-01"
			}
		},
		"db_table": "departments",
		"db_column": "d_created"
	}
}
